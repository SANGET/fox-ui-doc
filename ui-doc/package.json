  "author": "Sanget",
  "license": "MIT",
  "keywords": [
    "react-doc-generator"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,md,mdx}\"",
    "build": "npm run clean:build; tsc; npm run cp:files",
    "cp:files": "sh ./scripts/copy-files.sh",
    "clean:build": "rm -rf ./dist",
    "release": "npm run build; npm run publish:dist",
    "publish:dist": "npm publish ./dist --access public"
  },
