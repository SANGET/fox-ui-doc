    "@mdx-js/react": "^1.5.1",
    "@types/styled-components": "^4.1.19",
    "@deer-ui/doc-generator": "*",
    "babel-plugin-styled-components": "^1.10.0",
    "deepmerge": "^4.2.1",
    "fs-extra": "^8.1.0",
    "gatsby-image": "^2.2.29",
    "gatsby-plugin-compile-es6-packages": "^2.1.0",
    "gatsby-plugin-google-analytics": "^2.1.19",
    "gatsby-plugin-manifest": "^2.2.20",
