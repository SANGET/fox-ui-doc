    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-transformer-sharp": "^2.3.0",
    "github-slugger": "^1.2.1",
    "mdast-util-to-string": "^1.0.5",
    "polished": "^3.2.0",
    "prism-react-renderer": "^1.0.2",
    "prismjs": "^1.16.0",
